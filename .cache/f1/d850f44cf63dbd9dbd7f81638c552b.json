seReducer(ArticlesReducer, initialArticles);\n  const [error, setError] = useState(\"\"); //Used to show any errors.\n  const navigate = useNavigate(); //Used to redirect the user.\n\n  function ERROR(msg) {\n    //Used to show any errors.\n    setError(msg); //Update the state to show the error\n\n    setTimeout(() => {\n      //After some time,\n      setError(\"\"); //clear the error\n    }, 2000);\n  }\n\n  function logout() {\n    //When logging out\n    localStorage.removeItem(\"token\"); //remove the token from the storage\n    navigate(\"/\"); //redirect the user to the homepage\n  }\n\n  return (\n    //html\n    <div className=\"App\">\n      <button id=\"logout\" onClick={logout}>\n        Logout\n      </button>\n      <h1> Client-Side Auth </h1> {/* Tittle*/}\n      <nav>\n        {/* Navigation*/}\n        <Link id=\"loginScreen\" to=\"/\">\n          Login\n        </Link>\n        <Link id=\"articlesScreen\" to=\"/articles\">\n          Articles\n        </Link>\n      </nav>\n      <p id=\"error\">{error}</p>\n      <Routes>\n        {/* Routes*/}\n        <Route\n          path=\"/\"\n          element={<LoginForm navigate={navigate} Error={ERROR} />}\n        />\n\n        <Route\n          exact\n          path=\"articles\"\n          element={\n            <>\n              <ArticleForm\n                navigate={navigate}\n                Error={ERROR}\n                Articles={articles}\n                setArticles={setArticles}\n              />\n              <Articles articles={articles} setArticles={setArticles} />\n            </>\n          }\n        />\n      </Routes>\n    </div>\n  );\n}\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"5241a45d2e4a17716cd84682e8da0012","cacheData":{"env":{}}}