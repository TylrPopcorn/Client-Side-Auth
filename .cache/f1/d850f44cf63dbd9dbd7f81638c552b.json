ated":{"line":72,"column":2},"source":"components/App.js","original":{"line":72,"column":10}},{"generated":{"line":73,"column":0},"source":"components/App.js","original":{"line":74,"column":0}},{"generated":{"line":74,"column":0},"source":"components/App.js","original":{"line":74,"column":1}},{"generated":{"line":74,"column":15},"source":"components/App.js","original":{"line":76,"column":15},"name":"App"},{"generated":{"line":74,"column":18},"source":"components/App.js","original":{"line":76,"column":18}},{"generated":{"line":75,"column":0},"source":"components/App.js","original":{"line":76,"column":18}}],"sources":{"components/App.js":"import React, { useState } from \"react\";\nimport { Link, Routes, Route, useNavigate } from \"react-router-dom\";\n\nimport { initialArticles } from \"./Articles\";\n//------\nimport Articles from \"./Articles\";\nimport LoginForm from \"./LoginForm\";\nimport ArticleForm from \"./ArticleForm\";\n\nfunction App() {\n  //Main function\n  const [articles, setArticles] = useState(initialArticles); //Used to hold all of the articles.\n  const [error, setError] = useState(\"\"); //Used to show any errors.\n  const navigate = useNavigate(); //Used to redirect the user.\n\n  function ERROR(msg) {\n    //Used to show any errors.\n    setError(msg); //Update the state to show the error\n\n    setTimeout(() => {\n      //After some time,\n      setError(\"\"); //clear the error\n    }, 2000);\n  }\n\n  function logout() {\n    //When logging out\n    localStorage.removeItem(\"token\"); //remove the token from the storage\n    navigate(\"/\"); //redirect the user to the homepage\n  }\n\n  return (\n    //html\n    <div className=\"App\">\n      <button id=\"logout\" onClick={logout}>\n        Logout\n      </button>\n      <h1> Client-Side Auth </h1> {/* Tittle*/}\n      <nav>\n        {/* Navigation*/}\n        <Link id=\"loginScreen\" to=\"/\">\n          Login\n        </Link>\n        <Link id=\"articlesScreen\" to=\"/articles\">\n          Articles\n        </Link>\n      </nav>\n      <p id=\"error\">{error}</p>\n      <Routes>\n        {/* Routes*/}\n        <Route\n          path=\"/\"\n          element={<LoginForm navigate={navigate} Error={ERROR} />}\n        />\n\n        <Route\n          exact\n          path=\"articles\"\n          element={\n            <>\n              <ArticleForm\n                navigate={navigate}\n                Error={ERROR}\n                Articles={articles}\n                setArticles={setArticles}\n              />\n              <Articles articles={articles} />\n            </>\n          }\n        />\n      </Routes>\n    </div>\n  );\n}\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"b924a417915c9ad271465525e768cb0e","cacheData":{"env":{}}}